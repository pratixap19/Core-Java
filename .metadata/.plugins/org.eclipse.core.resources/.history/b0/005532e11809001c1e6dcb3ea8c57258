
public class ReverseString {

	public static void main(String[] args) {
		
		String name = "Selenium";
		String rev="";
		int len=name.length();
		System.out.println(len);
		
		for(int i=len-1;i>=0;i--) 
		{
			rev = rev+name.charAt(i);
		}
         System.out.println("Reverse of string is " + rev);
	}

}
