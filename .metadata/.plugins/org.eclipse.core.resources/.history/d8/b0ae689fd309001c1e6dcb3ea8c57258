
public class Palindromnumber {
//1. Check whether string “ann” is palindrome or not? If the string is not palindrome then make it as palindrome.
	// 2. Return the maximun occurance of character from the string “aabbdcccccdd”

	static boolean isPalindrom(String str)
	{
		int i = 0, j = str.length() - 1;
		while (i < j) {
			if (str.charAt(i) != str.charAt(j))
				return false;
			i++;
			j--;
		}
		return true;
	}



	public static void main(String[] args) {

		String str = "ann";
		if (isPalindrom(str))
			System.out.println("Yes");
		else
			System.out.println("No");
	}

}
